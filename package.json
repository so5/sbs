{
  "name": "simple-batch-system",
  "version": "1.14.0",
  "description": "simple batch system provides job schduler like async function queueing system.",
  "main": "lib/index.js",
  "repository": "github:so5/sbs",
  "scripts": {
    "mocha": "mocha --recursive test",
    "lint": "eslint --fix lib test",
    "test": "nyc --reporter=html --reporter=text mocha",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "check": "run-s lint test",
    "postversion": "git push origin $(git describe --abbrev=0 --tags)"
  },
  "author": {
    "name": "Naoyuki Sogo"
  },
  "license": "MIT",
  "devDependencies": {
    "ajv": "^6.11.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.0.9",
    "cross-env": "^7.0.0",
    "eslint": "^6.8.0",
    "eslint-config-eslint": "^6.0.0",
    "eslint-plugin-chai-expect": "^2.1.0",
    "eslint-plugin-chai-friendly": "^0.5.0",
    "eslint-plugin-jsdoc": "^21.0.0",
    "eslint-plugin-node": "^11.0.0",
    "husky": "^4.3.6",
    "jsdoc-to-markdown": "^5.0.3",
    "lint-staged": "^10.5.3",
    "mocha": "^8.2.1",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.0.0",
    "rewire": "^4.0.1",
    "sinon": "^8.1.1",
    "sinon-chai": "^3.4.0"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "uuid": "^3.4.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test"
    }
  },
  "lint-staged": {
    "*.js": "eslint --fix"
  }
}
